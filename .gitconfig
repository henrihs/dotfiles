[user]
	name = Henrik Heggelund Svendsen
	email = hhe@dips.no
[core]
	editor = vim.exe
[push]
	default = current
[color]
	status = always
[core]
	autocrlf = false
	filemode = false
	filemode = false
	filemode = false
[alias]
	logtree = log --graph --oneline --decorate --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
	ls = log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	aliases = "!git config -l | grep alias | cut -c 7-"
	find = "!git ls-files | grep -i"
	logme = log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --author=Heggelund

[credential]
	helper = !'/cygdrive/C/Users/hhe/AppData/Roaming/GitCredStore/git-credential-winstore.exe'
